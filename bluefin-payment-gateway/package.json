{
	"name": "bluefin-payment-gateway",
	"version": "1.0.0",
	"author": "Bluefin",
	"license": "GPL-3.0",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build ./src/*",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "eslint . --ext=js,jsx,ts,tsx --fix",
		"lint:php": "./vendor/bin/phpcs --standard=phpcs.xml.dist -n $(git ls-files | grep .php$)",
		"lint:php-fix": "./vendor/bin/phpcbf --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest"
	},
	"dependencies": {
		"@wordpress/hooks": "^4.29.0",
		"@wordpress/i18n": "^6.2.0"
	},
	"devDependencies": {
		"@woocommerce/dependency-extraction-webpack-plugin": "^4.0.0",
		"@woocommerce/eslint-plugin": "^2.3.0",
		"@wordpress/scripts": "^24.6.0"
	}
}
